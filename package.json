{
  "name": "splittypie",
  "version": "0.0.0",
  "description": "Split your expenses with ease",
  "private": true,
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "start": "ember server",
    "test": "ember test",
    "deploy": "rm -rf dist/ && ember build && firebase use dev && firebase deploy",
    "deploy:prod": "rm -rf dist/ && ember build --environment=production && firebase use prod && firebase deploy",
    "deploy:staging": "rm -rf dist/ && ember build --environment=staging && firebase use staging && firebase deploy",
    "deploy:offline": "rm -rf dist/ && ember build --environment=offline && firebase use offline && firebase deploy",
    "seed": "firebase use dev && ./get-seed-json.js default | firebase database:set / -y",
    "seed:staging": "firebase use staging && ./get-seed-json.js default | firebase database:set / -y"
  },
  "repository": "",
  "engines": {
    "node": ">= 0.10.0"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "bower": "^1.7.1",
    "broccoli-asset-rev": "^2.2.0",
    "broccoli-serviceworker": "git://github.com/tsubik/broccoli-serviceworker#fallback-request-navigate-fix",
    "dotenv": "^2.0.0",
    "ember-ajax": "2.4.1",
    "ember-cli": "2.7.0",
    "ember-cli-app-version": "^1.0.0",
    "ember-cli-babel": "^5.1.5",
    "ember-cli-content-security-policy": "0.5.0",
    "ember-cli-dependency-checker": "^1.2.0",
    "ember-cli-deprecation-workflow": "0.2.3",
    "ember-cli-eslint": "^1.4.0",
    "ember-cli-htmlbars": "^1.0.1",
    "ember-cli-htmlbars-inline-precompile": "^0.3.1",
    "ember-cli-ic-ajax": "1.0.0",
    "ember-cli-inject-live-reload": "^1.3.1",
    "ember-cli-inline-content": "0.4.0",
    "ember-cli-moment-shim": "2.0.0",
    "ember-cli-qunit": "^2.0.2",
    "ember-cli-release": "1.0.0-beta.2",
    "ember-cli-sass": "5.4.0",
    "ember-cli-sri": "^2.0.0",
    "ember-cli-test-loader": "^1.1.0",
    "ember-cli-test-model-waiter": "0.1.1",
    "ember-cli-uglify": "^1.2.0",
    "ember-data": "2.7.0",
